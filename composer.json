{
    "name": "amirhshokri/laravel-filterable",
    "description": "A better solution to add filter functionality to laravel query builder.",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Amirhshokri\\LaravelFilterable\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Amir Hossein Shokri",
            "email": "shokridev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/console": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amirhshokri\\LaravelFilterable\\Providers\\LaravelFilterableServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
